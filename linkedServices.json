[
    {
        "name": "AzureKeyVaultLinkedService",
        "properties": {
            "type": "AzureKeyVault",
          "typeProperties": {
            "baseUrl": "https://keyvault-abnarain.vault.azure.net/"
          }
        }
    },
    {
        "name": "AzureStorageLinkedService",
        "properties": {
            "type": "AzureStorage",
            "typeProperties": {
              "connectionString": {
                "type": "AzureKeyVaultSecret",
                "store": {
                  "referenceName": "AzureKeyVaultLinkedService",
                  "type": "LinkedServiceReference"
                },
                "secretName": "contosostorage-connectionstring"
              }
            }
        }
    }
]
